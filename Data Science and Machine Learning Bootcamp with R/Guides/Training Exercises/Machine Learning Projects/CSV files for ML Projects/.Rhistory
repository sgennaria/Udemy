cm.report <- function(cm) {
accuracy <- (cm[1,1] + cm[2,2]) / sum(cm)
error.rate <-(cm[1,2] + cm[2,1]) / sum(cm)
recall <- cm[2,2] / sum(cm[2,])
precision <- cm[2,2] / sum(cm[,2])
cat('Accuracy: ', accuracy, '
Error Rate: ', error.rate, '
Recall: ', recall, '
Precision: ', precision)
}
